<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="Paths.props" />
  <PropertyGroup>
	<ProjectBuildDir>$(BuildDir)\$(MSBuildProjectName)</ProjectBuildDir>
	<OutputPath>$(ProjectBuildDir)\bin</OutputPath>
	<OutDir>$(OutputPath)</OutDir>
	<BaseIntermediateOutputPath>$(ProjectBuildDir)\obj</BaseIntermediateOutputPath>
    <PlatformTarget>AnyCPU</PlatformTarget>
    <NoWarn>1591</NoWarn>
    <DocumentationFile>$(OutputPath)\$(AssemblyName).xml</DocumentationFile>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <AssemblyName>$(MSBuildProjectName)</AssemblyName>
    <OutputType>Library</OutputType>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <RootNamespace>$(ProductName)</RootNamespace>
    <!-- 
      <SignAssembly>true</SignAssembly>
      <AssemblyOriginatorKeyFile>$(SolutionDir)key.snk</AssemblyOriginatorKeyFile>
    -->
    <TargetFrameworkVersion>v4.6.1</TargetFrameworkVersion>
    <RestorePackages>true</RestorePackages>
    <DocumentationFile>$(OutputPath)\$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug|AnyCPU'">
    <DebugSymbols>true</DebugSymbols>
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Release|AnyCPU'">
    <DefineConstants>TRACE</DefineConstants>
    <Optimize>true</Optimize>
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <CodeAnalysisDictionary Include="CustomDictionary.xml" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="$(BuildDir)\..\CommonAssemblyInfo.cs" />
  </ItemGroup>
</Project>
